const $ = require('../utils/request_util')


const loadRecruitCompanyById = function (id) {
    return $.request({
        url: '/recruit-company/info?id=' + id,
        data: {},
        method: $.RequestMethod.GET,
        header: $.jsonHeader
    })
}

const submitRecruitCompany = function( recruitCompany){
    return $.request({
        url: '/recruit-company/modify',
        data: recruitCompany,
        method: $.RequestMethod.POST,
        header: $.jsonHeader,
    });
}


module.exports = {
    loadRecruitCompanyById: loadRecruitCompanyById,
    submitRecruitCompany: submitRecruitCompany,
}