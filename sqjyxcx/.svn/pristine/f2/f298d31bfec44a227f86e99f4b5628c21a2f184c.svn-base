<view class="container">
	<view class='top_nav'>
		<view class="nav-name {{item.typeId == currentId ? 'nav-hover':''}}" wx:for="{{section}}" wx:key="id" id='{{item.typeId}}' catchtap='handleTap'>
			{{item.name}}({{item.num}})
			<view class="linbc"></view>
		</view>
	</view>
	<swiper current="{{currentTab}}" data-current="{{currentId}}"  style="height:{{winHeight - 55}}px;" bindchange="switchTab">
			<!-- 待查看 -->
			<swiper-item style="height:100%;overflow-y:scroll;background: #fff;" >
				<block wx:for="{{joblist}}" wx:key="key">
					<view class="jobbox clearfloat" >
						<view class="ft ltbox">
							<view class="jobname">{{item.jobname}}</view>
							<view class="jobmoney">{{item.jobmoney}}元/月</view>
							<view class="companyname">
								<image src="{{item.companytx}}" class="companytx"></image>
								{{item.companyname}}
							</view>
						</view>
						<view class="fr lrbox">
							<view class="far">{{item.jl}}公里</view>
							<view class="phonecall" bindtap="callphone" data-phonenum="{{item.phonenum}}">电话沟通</view>
						</view>
					</view>
					<view class="line1"></view>
				</block>
			</swiper-item>

			<!-- 沟通中 -->
			<swiper-item style="height:100%;overflow-y:scroll;background: #fff;" >
				<block wx:for="{{jobgtlist}}" wx:key="key">
					<view class="jobbox clearfloat" >
						<view class="ft ltbox">
							<view class="jobname">{{item.jobname}}</view>
							<view class="jobmoney">{{item.jobmoney}}元/月</view>
							<view class="companyname">
								<image src="{{item.companytx}}" class="companytx"></image>
								{{item.companyname}}
							</view>
						</view>
						<view class="fr lrbox">
							<view class="far">{{item.jl}}公里</view>
							<view class="phonecall" bindtap="callphone" data-phonenum="{{item.phonenum}}">电话沟通</view>
						</view>
					</view>
					<view class="line1"></view>
				</block>
			</swiper-item>

			<!-- 不合适 -->
			<swiper-item style="height:100%;overflow-y:scroll;background: #fff;" >
				<block wx:for="{{jobbhslist}}" wx:key="key">
					<view class="jobbox clearfloat" >
						<view class="ft ltbox">
							<view class="jobname">{{item.jobname}}</view>
							<view class="jobmoney">{{item.jobmoney}}元/月</view>
							<view class="companyname">
								<image src="{{item.companytx}}" class="companytx"></image>
								{{item.companyname}}
							</view>
						</view>
						<view class="fr lrbox">
							<view class="far">{{item.jl}}公里</view>
							<view class="phonecall" bindtap="callphone" data-phonenum="{{item.phonenum}}">电话沟通</view>
						</view>
					</view>
					<view class="line1"></view>
				</block>
			</swiper-item>
	</swiper>
</view>




