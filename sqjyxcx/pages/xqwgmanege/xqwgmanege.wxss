.seatings {
  margin: 0;
  padding: 0;
}
.seatNormal, .seatExcellent, .seatNone, .seatChosen {
  height: 8vw;
  width: 14vw;
  margin: 1vw;
  border-radius: 8rpx;
  box-sizing: border-box;
}
.seatNormal {
  border: 1rpx solid #63c0c0;
}
.seatChosen {
  border: 1rpx solid red;
  background-color: red;
}
.selected {
  border: 1rpx solid #05ca90;
  background-color: #05ca90;
}
.seatExcellent {
  border: 1rpx solid #f18e14;
}
.seatNone {
  border: 1rpx solid rgba(0, 0, 0, 0);
}

.seatings_signs {
  display: flex;
  height: 80rpx;
  margin-left: 20rpx;
}
.signs_normal, .signs_excellent {
  display: flex;
  margin-right: 20rpx;
}
.emptyBox {
  height: 10vw;
  width: 100%;
}
.seatings_visual {
  width: 100%;
  height: 80vw;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.visual_screen {
  height: 30rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  overflow: hidden;
  margin-bottom: 10rpx;
}
.screen {
  margin-top: 0;
  padding: 0;
  height: 30vw;
  width: 100vw;
  box-sizing: border-box;
  border: 15rpx solid #c9cdd3;
  border-radius: 50%;
}
.visual_title {
  font-size: 23rpx;
  width: 100%;
  height: 20rpx;
  text-align: center;
  color: #6d6d6d;
  margin-bottom: 30rpx;
}
.visual_seatings {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 43vw;
}
.visual_seating {
  width: 90%;
  height: 43vw;
  /* display: flex; */
  flex-wrap: wrap;
}
